{
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "editor.fontFamily": "Fira Code Light",
  "editor.fontSize": 15,
  "editor.fontLigatures": true,
  "terminal.integrated.fontFamily": "Fira Code Light",
  "terminal.integrated.fontSize": 15,
  "git.confirmSync": false,
  "breadcrumbs.enabled": true,
  "extensions.ignoreRecommendations": false,
  "workbench.iconTheme": "eq-material-theme-icons",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "workbench.startupEditor": "newUntitledFile",
  "php.suggest.basic": false,
  "window.titleBarStyle": "custom",
  "gitlens.views.fileHistory.location": "scm",
  "workbench.colorTheme": "GitHub Dark",
  "kickassembler.assemblerJar": "{{ansible_env.HOME}}/bin/KickAssembler/KickAss.jar",
  "kickassembler.emulatorRuntime": "/usr/bin/x64sc",
  "kickassembler.javaRuntime": "/usr/bin/java",
  "kickassembler.outputDirectory": "build",
  "kickassembler.debuggerRuntime": "{{ansible_env.HOME}}/bin/C64-65XE-Debugger/c64debugger",
  "kickassembler.debuggerOptions": "-wait 3000 -autojmp",
  "editor.renameOnType": true
}
