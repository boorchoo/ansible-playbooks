{
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "editor.fontFamily": "Fira Code Light",
  "editor.fontSize": 15,
  "editor.fontLigatures": true,
  "editor.linkedEditing": true,
  "terminal.integrated.fontFamily": "Fira Code Light",
  "terminal.integrated.fontSize": 15,
  "breadcrumbs.enabled": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorTheme": "GitHub Dark",
  "workbench.iconTheme": "material-icon-theme",
  "window.titleBarStyle": "custom",
  "extensions.ignoreRecommendations": false,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "php.suggest.basic": false,
  "go.toolsManagement.autoUpdate": true,
  "kickassembler.assembler.jar": "{{ansible_env.HOME}}/bin/KickAssembler/KickAss.jar",
  "kickassembler.emulatorRuntime": "/usr/bin/x64sc",
  "kickassembler.javaRuntime": "/usr/bin/java",
  "kickassembler.outputDirectory": "build",
  "kickassembler.debuggerRuntime": "{{ansible_env.HOME}}/bin/C64-65XE-NES-Debugger/c64debugger",
  "kickassembler.debuggerOptions": "-wait 3000 -autojmp",
  "material-icon-theme.saturation": 0.8
}
